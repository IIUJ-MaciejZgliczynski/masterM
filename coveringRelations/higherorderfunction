#ifndef
#define HIGHERORDERFUNCTION

template <typename T, typename U>
void map(T & op, std::vector<U> & source, std::vector<U> & output){
	for(std::vector<U>::iterator it = source.begin() ; it!= source.end() ; ++it) {
		output.push_back(op(*it));
	}
}

template <typename T, typename U>
void map(T & op, std::vector<U> & source, std::vector<U> & output){
	for(std::vector<U>::iterator it = source.begin() ; it!= source.end() ; ++it) {
		if(op(*it))
			output.push_back(*it);
	}
}


#endif
